generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Payment {
  id        Int      @id @default(autoincrement())
  vehicleId Int
  amount    Float
  method    String
  timestamp DateTime @default(now())
}
