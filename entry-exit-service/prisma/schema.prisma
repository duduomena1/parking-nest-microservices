generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Vehicle {
  id           Int       @id @default(autoincrement())
  licensePlate String    @unique
  model        String
  color        String
  entryTime    DateTime  @default(now())
  exitTime     DateTime?
}
